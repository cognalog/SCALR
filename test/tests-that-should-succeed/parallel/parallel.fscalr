func first_ver (scale)
  [note.pitch(scale[1]).quarter, note.pitch(scale[2]).quarter, note.pitch(scale[3]).quarter, note.pitch(scale[4]).quarter]
end

func second_ver (scale)
  [note.pitch(scale[5]).quarter, note.pitch(scale[6]).quarter, note.pitch(scale[7]).quarter, note.pitch(scale[8]).quarter]
end